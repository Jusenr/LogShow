<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.putao.logshow">

    <uses-sdk tools:overrideLibrary="com.appstore.verificationuilibrary,com.putao.minisdk,com.putao.ptx.ptuilib"/>

    <uses-permission android:name="com.putao.ptlog.provider"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pt_002"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pt_002"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".TestActivity">
        </activity>
        <activity android:name=".Test2Activity">
        </activity>
    </application>

</manifest>